{
  "name": "mono",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "personal-web": "pnpm --filter personal-web",
    "pw": "pnpm personal-web",
    "copy-anything": "pnpm --filter copy-anything",
    "ca": "pnpm copy-anything",
    "common": "pnpm --filter @jkaifer/common",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "casa:scan": "docker run -v .:/my-dir fluidattacks/cli:amd64 skims scan /my-dir/fluidattacks-config.yaml"
  },
  "keywords": [],
  "author": "Jan Kaifer",
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18.16.1"
  },
  "packageManager": "pnpm@8.15.1",
  "type": "module",
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@nx/eslint": "18.0.6",
    "@nx/next": "18.0.6",
    "@nx/vite": "18.0.6",
    "@nx/web": "18.0.6",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@vitest/ui": "1.3.1",
    "add": "2.0.6",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "husky": "9.0.11",
    "nx": "18.0.6",
    "pnpm": "8.15.4",
    "prettier": "3.2.5",
    "vite": "5.1.4",
    "vitest": "1.3.1"
  }
}
